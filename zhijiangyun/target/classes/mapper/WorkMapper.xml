<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjy.mapper.WorkMapper">

    <select id="getWorkList" resultType="WorkPart">
        select * from work;
    </select>

    <select id="getFirstWorkList" resultType="WorkPart">
        select * from work limit 0,5;
    </select>

    <select id="getSecondWorkList" resultType="WorkPart">
        select * from work limit 5,5;
    </select>

</mapper>
